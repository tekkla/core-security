        {
            "Core\\Security": ""
        }
    },

    "type": "library",
    "homepage": "https://github.com/tekkla/core-security",
    "authors":
    [
        {
            "name": "Michael Zorn",
            "email": "tekkla@tekkla.de",
            "role": "Developer"
        }
